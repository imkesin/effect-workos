{
  "name": "@effect-workos/server",
  "version": "0.1.0",
  "dependencies": {
    "@effect-workos/lib": "workspace:*",
    "@effect-workos/workos": "workspace:*",
    "@effect/platform": "^0.93.6",
    "@effect/platform-node": "^0.103.0",
    "@workos-inc/node": "^7.77.0",
    "effect": "^3.19.10"
  },
  "keywords": [],
  "license": "ISC",
  "private": true,
  "scripts": {
    "build": "tsc",
    "clean": "rm -rf .turbo dist",
    "dev": "run-p dev:**",
    "dev:node": "npm run node:base -- --watch dist/dev.js",
    "dev:tsc": "tsc --watch --preserveWatchOutput",
    "lint": "eslint .",
    "node:base": "node --env-file=.env.local --enable-source-maps",
    "start": "npm run node:base -- dist/main.js",
    "test": "vitest",
    "test:integration": "vitest --config ./vitest.integration.config.ts",
    "test:once": "vitest run",
    "typecheck": "run-p typecheck:**",
    "typecheck:main": "tsc -p tsconfig.json --noEmit",
    "typecheck:test": "tsc -p tsconfig.test.json --noEmit"
  },
  "type": "module"
}
